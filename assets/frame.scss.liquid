		display:block;
}
	}
  .authorimg {
	margin-bottom: 10px;
	padding: 0 15px;
	position: relative;
	min-width: 370px;
    img {
	display: inline-block;
	border-radius: 8px;
	margin: 0px 0;
	height: 280px;
	width: 280px;
}
    @include at-query ($max, $medium) {
      min-width: auto;
    }
    &:before {
       @include at-query ($min, $large) {
      content: '';
		position: absolute;
		top: -60px;
		right: 10px;
		width: 170px;
		height: 110px;
		z-index: -1;
		background: #f3f3f3;
		display: block;
		-webkit-border-radius: 3px;
		border-radius: 3px;
    }
    }
    
    &:after {
       @include at-query ($min, $large) {
      content: '';
		position: absolute;
		left: 0;
		bottom: -45px;
		width: 170px;
		height: 150px;
		z-index: -1;
		background: #f3f3f3;
		-webkit-border-radius: 3px;
		border-radius: 3px;
    }
    }
    
}
  .vc-content-nameposition {
	padding: 0 15px;
	width: 100%;
	text-align: left;
    font-size: 18px;
    @include at-query  ($max,$postlarge) {font-size: 16px; }
}
  
}





