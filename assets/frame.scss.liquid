/* Swiper */
.swiper-slide {
	opacity: 0;
	width: auto;
	visibility: hidden;
	transition: opacity 200ms ease-in-out, visibility 200ms ease-in-out;
}

.swiper-slide-active {
	opacity: 1;
	visibility: visible;
}

.swiper-slide-active+.swiper-slide {
	opacity: 1;
	visibility: visible;
}

.swiper-pagination {
	position: unset;
}

